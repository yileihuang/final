<%= form_for @visit do |form| %>
<div class="form-group">
 <%= form.label :group_id %>
    <%= form.collection_select(:group_id, Group.all, :id, :name) %>
</div>
<div class="form-group">
 <%= form.label :touristsite_id %>
    <%= form.collection_select(:touristsite_id, Touristisite.all, :id, :name) %>
</div>
  <%= form.button 'Update Visit', class: "btn btn-success" %>
<% end %>